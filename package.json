    "@openzeppelin/contracts": "4.5.0",
    "aws-sdk": "^2.1095.0",
    "chart.js": "^3.7.1",
    "concurrently": "^7.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "7.0.3",
