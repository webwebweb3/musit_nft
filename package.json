{
  "name": "musit_nft",
  "version": "1.0.0",
  "description": "musit_nft",
  "main": "server/app.js",
  "scripts": {
    "start": "node server/app.js",
    "test_backend": "cross-env NODE_ENV=test node server/app.js",
    "nodemon": "nodemon server/app.js",
    "test_nodemon": "cross-env NODE_ENV=test nodemon server/app.js",
    "frontend": "npm run dev --prefix client",
    "dev": "concurrently \"npm start\" \"npm run frontend\"",
    "dev_nodemon": "concurrently \"npm run nodemon\" \"npm run frontend\"",
    "test": "concurrently \"npm run test_backend\" \"npm run frontend\"",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webwebweb3/musit_nft.git"
  },
  "author": "WebWebWeb",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webwebweb3/musit_nft/issues"
  },
  "homepage": "https://github.com/webwebweb3/musit_nft#readme",
  "dependencies": {
    "aws-sdk": "^2.1095.0",
    "babel-plugin-styled-components": "^2.0.6",
    "concurrently": "^7.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.4",
    "multer-s3": "^2.10.0",
    "mysql2": "^2.3.3",
    "passport": "^0.5.2",
    "passport-local": "^1.0.0",
    "prop-types": "^15.8.1",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1"
  },
  "devDependencies": {
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "husky": "^7.0.0",
    "lint-staged": "^12.3.5",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
